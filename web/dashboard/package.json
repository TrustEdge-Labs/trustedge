    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "json-schema-to-typescript": "^15.0.4",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^3.5.0",
    "svelte": "^5.53.2",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
